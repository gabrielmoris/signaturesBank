{{>navbar}}
<h1>All the signatures of our Database:</h1>
<ol>
    {{#signatures}}
        <li>
            {{#if this.url}}
                <a class="signers" href={{url}} target="_blank">{{first}}
                    {{last}}</a>
                <p class="signers"> {{age}}</p><a
                    class="signers"
                    href="/signatures/{{city}}"
                >{{city}}</a>{{else}}
                <p class="signers">{{first}} {{last}}</p><p class="signers">{{age}}</p>
                <a class="signers" href="/signatures/{{city}}">{{city}}</a>
            {{/if}}</li>
    {{/signatures}}
</ol>
<div></div>
<div></div>